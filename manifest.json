{
    "manifest_version": 3,
    "name": "Make the Internet Great Again",
    "version": "1.0.0",
    "description": "Description to be changed",
    "permissions": ["activeTab", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content_scripts/content.js"]
        }
    ],
    "action": {
        "default_popup": "popup/popup.html"
    }
}